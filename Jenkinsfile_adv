
pipeline {
    agent { label 'Slave_test' }

    stages {
        stage('Read and Print test') {
            steps {
                bat """
                    echo Hello from Windows agent
                    dir C:\\
                """
            }
        }

        stage('End test') {
            steps {
                bat """
                    echo End test from Windows agent
                """
            }
        }
    }
}